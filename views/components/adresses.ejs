<table class="tableau">
 <tr>
 
 <th><a href='/trier/nom/asc'>Nom</a></th> 
 <th><a href='/trier/prenom/asc'>Prenom</a></th>
 <th><a href='/trier/telephone/asc'>Telephone</a></th>
 <th><a href='/trier/courriel/asc'>Courriel</a></th>
     
<th><a href='/trier/id/asc'>ID</a></th>
 <th>Modifier</th>
 <th>Ajouter</th> 
 </tr>
 <% adresse.forEach(elm => {%>
 <tr>
 
 <td contenteditable='true'><%= elm.nom %></td>
 <td contenteditable='true'><%= elm.prenom %></td>
 <td contenteditable='true'><%= elm.telephone %></td>
 <td contenteditable='true'><%= elm.courriel %></td>
<td><%= elm._id %></td>
 <td><a href="/supprimer/<%= elm._id %>">Supprimer</a></td>
 <td><a class='sauver' href="#">Sauver</a></td>
 <td><a href=/profil/<%=elm._id%>>Profil</a>
       </td>
 </tr>
 <% }) %>
 </table>
